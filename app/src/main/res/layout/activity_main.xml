<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="140dp"
        android:gravity="center_horizontal">

        <androidx.cardview.widget.CardView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:cardCornerRadius="10dp"
            android:layout_marginVertical="10dp">
            <ImageView
                android:id="@+id/twice_img_1"
                android:scaleType="center"
                android:src="@drawable/member_1"
                android:layout_width="120dp"
                android:layout_height="120dp" />
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:cardCornerRadius="10dp"
            android:layout_marginVertical="10dp"
            android:layout_marginLeft="10dp">
            <ImageView
                android:id="@+id/twice_img_2"
                android:scaleType="center"
                android:src="@drawable/member_2"
                android:layout_width="120dp"
                android:layout_height="120dp" />
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:cardCornerRadius="10dp"
            android:layout_marginVertical="10dp"
            android:layout_marginLeft="10dp">
            <ImageView
                android:id="@+id/twice_img_3"
                android:scaleType="center"
                android:src="@drawable/member_3"
                android:layout_width="120dp"
                android:layout_height="120dp" />
        </androidx.cardview.widget.CardView>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="140dp"
        android:gravity="center_horizontal">

        <androidx.cardview.widget.CardView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:cardCornerRadius="10dp"
            android:layout_marginVertical="10dp">
            <ImageView
                android:id="@+id/twice_img_4"
                android:scaleType="center"
                android:src="@drawable/member_4"
                android:layout_width="120dp"
                android:layout_height="120dp" />
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:cardCornerRadius="10dp"
            android:layout_marginVertical="10dp"
            android:layout_marginLeft="10dp">
            <ImageView
                android:id="@+id/twice_img_5"
                android:scaleType="center"
                android:src="@drawable/member_5"
                android:layout_width="120dp"
                android:layout_height="120dp" />
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:cardCornerRadius="10dp"
            android:layout_marginVertical="10dp"
            android:layout_marginLeft="10dp">
            <ImageView
                android:id="@+id/twice_img_6"
                android:scaleType="center"
                android:src="@drawable/member_6"
                android:layout_width="120dp"
                android:layout_height="120dp" />
        </androidx.cardview.widget.CardView>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="140dp"
        android:gravity="center_horizontal">

        <androidx.cardview.widget.CardView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:cardCornerRadius="10dp"
            android:layout_marginVertical="10dp">
            <ImageView
                android:id="@+id/twice_img_7"
